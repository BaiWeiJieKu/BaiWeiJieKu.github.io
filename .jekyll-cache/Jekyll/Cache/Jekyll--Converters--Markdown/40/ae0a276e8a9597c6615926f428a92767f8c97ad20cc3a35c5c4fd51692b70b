			  <span class="o">.</span><span class="na">setServiceName</span><span class="o">(</span><span class="s">"%sService"</span><span class="o">)</span>  <span class="c1">// è®¾ç½®ç”Ÿæˆçš„serviceæ¥å£çš„åå­—çš„é¦–å­—æ¯æ˜¯å¦ä¸ºI</span>
			  					   <span class="c1">// IEmployeeService</span>
 			  <span class="o">.</span><span class="na">setBaseResultMap</span><span class="o">(</span><span class="kc">true</span><span class="o">)</span>
 			  <span class="o">.</span><span class="na">setBaseColumnList</span><span class="o">(</span><span class="kc">true</span><span class="o">);</span>
		
		<span class="c1">//2. æ•°æ®æºé…ç½®</span>
		<span class="nc">DataSourceConfig</span>  <span class="n">dsConfig</span>  <span class="o">=</span> <span class="k">new</span> <span class="nc">DataSourceConfig</span><span class="o">();</span>
		<span class="n">dsConfig</span><span class="o">.</span><span class="na">setDbType</span><span class="o">(</span><span class="nc">DbType</span><span class="o">.</span><span class="na">MYSQL</span><span class="o">)</span>  <span class="c1">// è®¾ç½®æ•°æ®åº“ç±»å‹</span>
				<span class="o">.</span><span class="na">setDriverName</span><span class="o">(</span><span class="s">"com.mysql.jdbc.Driver"</span><span class="o">)</span>
				<span class="o">.</span><span class="na">setUrl</span><span class="o">(</span><span class="s">"jdbc:mysql://localhost:3306/mp"</span><span class="o">)</span>
				<span class="o">.</span><span class="na">setUsername</span><span class="o">(</span><span class="s">"root"</span><span class="o">)</span>
				<span class="o">.</span><span class="na">setPassword</span><span class="o">(</span><span class="s">"1234"</span><span class="o">);</span>
		 
		<span class="c1">//3. ç­–ç•¥é…ç½®</span>
		<span class="nc">StrategyConfig</span> <span class="n">stConfig</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">StrategyConfig</span><span class="o">();</span>
		<span class="n">stConfig</span><span class="o">.</span><span class="na">setCapitalMode</span><span class="o">(</span><span class="kc">true</span><span class="o">)</span> <span class="c1">//å…¨å±€å¤§å†™å‘½å</span>
				<span class="o">.</span><span class="na">setDbColumnUnderline</span><span class="o">(</span><span class="kc">true</span><span class="o">)</span>  <span class="c1">// æŒ‡å®šè¡¨å å­—æ®µåæ˜¯å¦ä½¿ç”¨ä¸‹åˆ’çº¿</span>
				<span class="o">.</span><span class="na">setNaming</span><span class="o">(</span><span class="nc">NamingStrategy</span><span class="o">.</span><span class="na">underline_to_camel</span><span class="o">)</span> <span class="c1">// æ•°æ®åº“è¡¨æ˜ å°„åˆ°å®ä½“çš„å‘½åç­–ç•¥</span>
				<span class="o">.</span><span class="na">setTablePrefix</span><span class="o">(</span><span class="s">"tbl_"</span><span class="o">)</span> <span class="c1">//è¡¨å‰ç¼€</span>
				<span class="o">.</span><span class="na">setInclude</span><span class="o">(</span><span class="s">"tbl_employee"</span><span class="o">);</span>  <span class="c1">// ç”Ÿæˆçš„è¡¨</span>
		
		<span class="c1">//4. åŒ…åç­–ç•¥é…ç½® </span>
		<span class="nc">PackageConfig</span> <span class="n">pkConfig</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">PackageConfig</span><span class="o">();</span>
		<span class="n">pkConfig</span><span class="o">.</span><span class="na">setParent</span><span class="o">(</span><span class="s">"com.atguigu.mp"</span><span class="o">)</span>
