		<span class="nc">AutoGenerator</span>  <span class="n">ag</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">AutoGenerator</span><span class="o">();</span>
		
		<span class="n">ag</span><span class="o">.</span><span class="na">setGlobalConfig</span><span class="o">(</span><span class="n">config</span><span class="o">)</span>
