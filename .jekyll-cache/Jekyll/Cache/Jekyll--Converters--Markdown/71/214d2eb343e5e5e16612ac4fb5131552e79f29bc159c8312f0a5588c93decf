        <span class="c1">//genericheros.add(new Item("å†°æ–"));</span>
         
        <span class="c1">//é™¤æ­¤ä¹‹å¤–ï¼Œè¿˜èƒ½å­˜æ”¾Heroçš„å­ç±»</span>
        <span class="n">genericheros</span><span class="o">.</span><span class="na">add</span><span class="o">(</span><span class="k">new</span> <span class="nc">APHero</span><span class="o">());</span>
        
        <span class="c1">//å¹¶ä¸”åœ¨å–å‡ºæ•°æ®çš„æ—¶å€™ï¼Œä¸éœ€è¦å†è¿›è¡Œè½¬å‹äº†ï¼Œå› ä¸ºé‡Œé¢è‚¯å®šæ˜¯æ”¾çš„Heroæˆ–è€…å…¶å­ç±»</span>
        <span class="nc">Hero</span> <span class="n">h</span> <span class="o">=</span> <span class="n">genericheros</span><span class="o">.</span><span class="na">get</span><span class="o">(</span><span class="mi">0</span><span class="o">);</span>
        
    <span class="o">}</span>
      
