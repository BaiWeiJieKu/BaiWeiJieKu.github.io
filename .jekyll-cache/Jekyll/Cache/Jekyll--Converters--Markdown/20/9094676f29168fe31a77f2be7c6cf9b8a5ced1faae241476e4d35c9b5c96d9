<span class="nc">Integer</span><span class="o">[]</span> <span class="n">data2</span> <span class="o">=</span> <span class="nc">Arrays</span><span class="o">.</span><span class="na">copyOf</span><span class="o">(</span><span class="n">data1</span><span class="o">,</span> <span class="mi">5</span><span class="o">);</span>
