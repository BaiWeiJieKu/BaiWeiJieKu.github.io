		<span class="c1">// è¾“å‡ºç»“æœï¼š993333666</span>
		<span class="k">for</span> <span class="o">(</span><span class="kt">int</span> <span class="n">i</span> <span class="o">:</span> <span class="n">h</span><span class="o">)</span> <span class="o">{</span>
